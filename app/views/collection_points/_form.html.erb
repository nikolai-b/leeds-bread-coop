<%= bootstrap_form_for(@collection_point) do |f| %>
    <%= f.text_area :address %>
    <%= f.text_area :post_code %>
    <%= f.text_area :notes %>
    <%= f.text_area :name %>
    <% button_label = @collection_point.persisted? ? "Update" : "Create" %>
    <%= f.submit button_label, class: "btn btn-success" %>
<% end %>
