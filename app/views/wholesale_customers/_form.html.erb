<%= bootstrap_form_for(@wholesale_customer) do |f| %>
    <%= f.text_field :name %>
    <%= f.text_area :address %>
    <%= f.text_field :phone %>
    <%= f.time_select :opening_time %>
    <%= f.check_box :delivery_receipt %>
    <% button_label = @wholesale_customer.persisted? ? "Update" : "Create" %>
    <%= f.submit button_label, class: "btn btn-success" %>
<% end %>
